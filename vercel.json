{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "MONGO_URI": "@env.MONGO_URI",
    "PORT": "@env.PORT",
    "DIMAINNAME": "@env.DIMAINNAME"
  }
}